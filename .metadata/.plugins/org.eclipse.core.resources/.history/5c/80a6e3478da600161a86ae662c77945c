		missingMessage:'请输入内容',
		invalidMessage:'输入有误',
		tipPosition:'right',
		deltaX:100,
		//noVlidate:true,
		
	});
	$(document),click(function(){
		console.log($('#ss').validatebox('validate'));
		console.log($('#ss').validatebox('isValid'))
	})
	});