<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="3.0" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name />
  <servlet>
    <servlet-name>action</servlet-name>
    <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
    <init-param>
      <param-name>config</param-name>
      <param-value>/WEB-INF/struts-config.xml</param-value>
    </init-param>
    <init-param>
      <param-name>debug</param-name>
      <param-value>3</param-value>
    </init-param>
    <init-param>
      <param-name>detail</param-name>
      <param-value>3</param-value>
    </init-param>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowYardsClServlet</servlet-name>
    <servlet-class>com.qdu.servlet.ShowYardsClServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowBuildingsClServlet</servlet-name>
    <servlet-class>com.qdu.servlet.ShowBuildingsClServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowEmployeeInfoSevlet</servlet-name>
    <servlet-class>com.qdu.servlet.ShowEmployeeInfoSevlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowStuClServlet</servlet-name>
    <servlet-class>com.qdu.servlet.ShowStuClServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowInspectionClServlet</servlet-name>
    <servlet-class>com.qdu.servlet.ShowInspectionClServlet</servlet-class>
  </servlet>



  <filter>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>com.qdu.filter.EncodingFilter</filter-class>
 
      <init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>



  <servlet-mapping>
    <servlet-name>action</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowYardsClServlet</servlet-name>
    <url-pattern>/ShowYardsClServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowBuildingsClServlet</servlet-name>
    <url-pattern>/ShowBuildingsClServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowEmployeeInfoSevlet</servlet-name>
    <url-pattern>/ShowEmployeeInfoSevlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowStuClServlet</servlet-name>
    <url-pattern>/ShowStuClServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowInspectionClServlet</servlet-name>
    <url-pattern>/ShowInspectionClServlet</url-pattern>
  </servlet-mapping>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>

